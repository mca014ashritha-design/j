pipeline {
    agent any

    triggers {
        pollSCM('* * * * *')
    }

    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/mca014ashritha-design/j'
            }
        }

        stage('Build') {
            steps {
                echo "Building..."
            }
        }



        stage('Test') {
            steps {
                echo "Testing..."
            }
        }
    }
}
