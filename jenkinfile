pipeline{
	agent any
      triggers{
cron('* * * * *')
}
		stages{
				stage('Checkout'){
						steps{
										git branch:'main', url:'https://github.com/mca014ashritha-design/j'
							}
					}
		stage('Bulid'){
				steps{
							echo "Building.."
				}
		}
		stage('Test'){
				steps{
					echo "Testing..."
				}
		}
	}
}
